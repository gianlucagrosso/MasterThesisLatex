	journal = {Constructive Approximation},
	number = {2},
	pages = {257--280},
	title = {O(dlog N)-Quantics Approximation of N-d Tensors in High-Dimensional Numerical Modeling},
	url = {https://doi.org/10.1007/s00365-011-9131-1},
	volume = {34},
